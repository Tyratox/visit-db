		"babel-preset-env": "^1.6.1",
		"body-parser": "^1.18.2",
		"celebrate": "^7.0.1",
		"compression": "^1.7.1",
		"ejs": "^2.5.7",
		"express": "^4.16.2",
		"express-session": "^1.15.6",
		"helmet": "^3.9.0",
		"opn": "^5.1.0",
		"sqlite3": "^3.1.13",
		"sync-request": "^4.1.0",
		"webpack": "^3.8.1",
		"webpack-node-externals": "^1.6.0",
		"winston": "^2.4.0"
