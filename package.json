	"bin": "build/compiled.js",
	"devDependencies": {
		"eslint": "^4.10.0",
		"pkg": "^4.3.0-beta.1",
		"typescript": "^2.5.3"
	},
	"scripts": {
		"start": "node src/index.js",
		"build": "webpack --config webpack.config.js && pkg --out-path build/ package.json",
